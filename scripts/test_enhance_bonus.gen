// 测试enhance_bonus问题
num(base_value) { 10 }
num(equipment_type) { 1 }
num(enhance_level) { 3 }

obj("Equipment", 1) {
    num(base_stat) { base_value }
    num(enhance_bonus) {
        num(bonus_per_level) { equipment_type * 2 }
        num(total_bonus) { bonus_per_level * enhance_level }
        total_bonus
    }
    num(final_value) { base_value + enhance_bonus }
}