// e3 简单循环语句

num(sum) { 0 }

obj("BreakContinueTest", 1) {
    // for(i, n)
    sum = 0
    for(i, 10) {
        sum = sum + i
    }
    num(sum_1) { sum }

    // for(i, start, end)
    sum = 0
    for(i, 5, 10) {
        sum = sum + i
    }
    num(sum_2) { sum }


    // for(i, start, end, step)
    sum = 0
    for(i, 1, 10, 2) {
        sum = sum + i
    }
    num(sum_3) { sum }
}

