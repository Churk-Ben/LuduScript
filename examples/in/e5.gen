// e5 嵌套分支与循环语句

obj("ControlFlowTest", 2) {
    num(nested_test) {
        num(total) { 0 }
        for(x, 1, 3) {
            if (x == 2) {
                for(y, 1, 2) {
                    total = total + y
                }
            }
            else {
                total = total + x
            }
        }
        total
    }
}